@model IEnumerable<Diabetes1.Models.AnalyzeGlycemic>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Recommend blood sugar lavel</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.symptom)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cause)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TakeCare)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.symptom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.cause)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TakeCare)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
            @Html.ActionLink("Details", "Details", new { id=item.id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
